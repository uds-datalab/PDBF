"move";divTop=divTop.replace("px","");divLeft=divLeft.replace("px","");var diffX=posX-divLeft,diffY=posY-divTop;document.onmousemove=function(evt){evt=evt||window.event;var posX=evt.clientX,posY=evt.clientY,aX=posX-diffX,aY=posY-diffY;if(aX<0)aX=0;if(aY<0)aY=0;if(aX+eWi>cWi)aX=cWi-eWi;if(aY+eHe>cHe)aY=cHe-eHe;mydragg.move(divid,aX,aY,container)}},stopMoving:function(container){document.getElementById(container).style.cursor="default";document.onmousemove=function(){}}}}();var debugmode=0;
