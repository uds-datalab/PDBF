"M 0 0"};c3_chart_internal_fn.transformForArcLabel=function(d){var $$=this,updated=$$.updateAngle(d),c,x,y,h,ratio,translate="";if(updated&&!$$.hasType("gauge")){c=this.svgArc.centroid(updated);x=isNaN(c[0])?0:c[0];y=isNaN(c[1])?0:c[1];h=Math.sqrt(x*x+y*y);ratio=$$.radius&&h?(36/$$.radius>.375?1.175-36/$$.radius:.8)*$$.radius/h:0;translate="translate("+x*ratio+","+y*ratio+")"}return translate};c3_chart_internal_fn.getArcRatio=function(d){var $$=this,whole=$$.hasType("gauge")?Math.PI:Math.PI*2;return d?
