function(event){context.destroy()};this.addAndTrackEvent(document,"mouseup",mouseUpHandler)}};Dygraph.prototype.drawZoomRect_=function(direction,startX,endX,startY,endY,prevDirection,prevEndX,prevEndY){var ctx=this.canvas_ctx_;if(prevDirection==Dygraph.HORIZONTAL)ctx.clearRect(Math.min(startX,prevEndX),this.layout_.getPlotArea().y,Math.abs(startX-prevEndX),this.layout_.getPlotArea().h);else if(prevDirection==Dygraph.VERTICAL)ctx.clearRect(this.layout_.getPlotArea().x,Math.min(startY,prevEndY),this.layout_.getPlotArea().w,
