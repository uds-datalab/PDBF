mozL10n.get("unexpected_response_error",null,"Unexpected server response.");var moreInfo={message:message};self.error(loadingErrorMessage,moreInfo);self.loading=false});if(args&&args.length)DocumentProperties.setFileSize(args.length)},download:function pdfViewDownload(){function downloadByUrl(){downloadManager.downloadUrl(url,filename)}var url=this.url.split("#")[0];var filename=getPDFFileNameFromURL(url);var downloadManager=new DownloadManager;downloadManager.onerror=function(err){PDFViewerApplication.error("PDF failed to download.")};
