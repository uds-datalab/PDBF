2)tickValues=[values[0],values[values.length-1]];else if(targetCount>2){count=targetCount-2;start=values[0];end=values[values.length-1];interval=(end-start)/(count+1);tickValues=[start];for(i=0;i<count;i++){tickValue=+start+interval*(i+1);tickValues.push(forTimeSeries?new Date(tickValue):tickValue)}tickValues.push(end)}}if(!forTimeSeries)tickValues=tickValues.sort(function(a,b){return a-b});return tickValues};Axis.prototype.generateTransitions=function generateTransitions(duration){var $$=this.owner,
