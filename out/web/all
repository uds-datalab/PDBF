return function(d,i){var scale=isSub?$$.getSubYScale(d.id):$$.getYScale(d.id),y0=scale(0),offset=y0;targets.forEach(function(t){var values=$$.isStepType(d)?$$.convertValuesToStep(t.values):t.values;if(t.id===d.id||indices[t.id]!==indices[d.id])return;if(targetIds.indexOf(t.id)<targetIds.indexOf(d.id))if(values[i].value*d.value>=0)offset+=scale(values[i].value)-y0});return offset}};c3_chart_internal_fn.isWithinShape=function(that,d){var $$=this,shape=$$.d3.select(that),isWithin;if(!$$.isTargetToShow(d.id))isWithin=
