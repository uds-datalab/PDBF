self.fallback(PDFJS.UNSUPPORTED_FEATURES.forms)}})},setInitialView:function pdfViewSetInitialView(storedHash,scale){this.isInitialViewSet=true;document.getElementById("pageNumber").value=this.pdfViewer.currentPageNumber=1;if(PDFHistory.initialDestination){this.navigateTo(PDFHistory.initialDestination);PDFHistory.initialDestination=null}else if(this.initialBookmark){this.setHash(this.initialBookmark);PDFHistory.push({hash:this.initialBookmark},!!this.initialBookmark);this.initialBookmark=null}else if(storedHash)this.setHash(storedHash);
