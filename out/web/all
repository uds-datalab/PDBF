getDygraphsOptions(json.jsonBig,rawZoomFactor,json.chartdataBig.columns);json.dygraphBig=new Dygraph(containerContent,json.chartdataBig.values,optionsBig)}else json.dygraphBig.updateOptions({logscale:logScale.checked,includeZero:includeZero.checked,drawPoints:drawPoints.checked,fillGraph:fillGraph.checked,showRangeSelector:showRangeSelector.checked})};var jsonCpy=jQuery.extend(true,{},json);var chartdataCpy=getChartData(json);json.resultBig=chartdataCpy.res;var tip="Tip: Click-and drag to zoom the graph. Shift + click-and drag to pan the graph.<br/>";
