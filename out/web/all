12).attr("width",maxWidth*(step+1)+10);$$.legend.selectAll("."+CLASS.legendItem).classed(CLASS.legendItemHidden,function(id){return!$$.isTargetToShow(id)});$$.updateLegendItemWidth(maxWidth);$$.updateLegendItemHeight(maxHeight);$$.updateLegendStep(step);$$.updateSizes();$$.updateScales();$$.updateSvgSize();$$.transformAll(withTransitionForTransform,transitions);$$.legendHasRendered=true};function Axis(owner){API.call(this,owner)}inherit(API,Axis);Axis.prototype.init=function init(){var $$=this.owner,
