startX,endX,startY,endY,0,0,0]};Dygraph.prototype.clearZoomRect_=function(){this.currentZoomRectArgs_=null;this.canvas_ctx_.clearRect(0,0,this.width_,this.height_)};Dygraph.prototype.doZoomX_=function(lowX,highX){this.currentZoomRectArgs_=null;var minDate=this.toDataXCoord(lowX);var maxDate=this.toDataXCoord(highX);this.doZoomXDates_(minDate,maxDate)};Dygraph.prototype.doZoomXDates_=function(minDate,maxDate){var old_window=this.xAxisRange();var new_window=[minDate,maxDate];this.zoomed_x_=true;var that=
