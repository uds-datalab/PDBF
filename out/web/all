0;if(isInitialBookmark){var target=window.history.state.target;if(!target){this._pushToHistory(params,false);this.previousHash=window.location.hash.substring(1)}this.updatePreviousBookmark=this.nextHashParam?false:true;if(target)this._updatePreviousBookmark();return}if(this.nextHashParam)if(this.nextHashParam===params.hash){this.nextHashParam=null;this.updatePreviousBookmark=true;return}else this.nextHashParam=null;if(params.hash)if(this.current.hash)if(this.current.hash!==params.hash)this._pushToHistory(params,
