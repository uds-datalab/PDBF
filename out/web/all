else if(args.rows)data=$$.convertRowsToData(args.rows);else if(args.columns)data=$$.convertColumnsToData(args.columns);else return;targets=$$.convertDataToTargets(data,true);$$.data.targets.forEach(function(t){var found=false,i,j;for(i=0;i<targets.length;i++)if(t.id===targets[i].id){found=true;if(t.values[t.values.length-1])tail=t.values[t.values.length-1].index+1;length=targets[i].values.length;for(j=0;j<length;j++){targets[i].values[j].index=tail+j;if(!$$.isTimeSeries())targets[i].values[j].x=tail+
j}t.values=t.values.concat(targets[i].values);targets.splice(i,1);break}if(!found)notfoundIds.push(t.id)});$$.data.targets.forEach(function(t){var i,j;for(i=0;i<notfoundIds.length;i++)if(t.id===notfoundIds[i]){tail=t.values[t.values.length-1].index+1;for(j=0;j<length;j++)t.values.push({id:t.id,index:tail+j,x:$$.isTimeSeries()?$$.getOtherTargetX(tail+j):tail+j,value:null})}});if($$.data.targets.length)targets.forEach(function(t){var i,missing=[];for(i=$$.data.targets[0].values[0].index;i<tail;i++)missing.push({id:t.id,
